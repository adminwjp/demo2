<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.shop.user.entity">
    <class name="com.shop.user.entity.Teambuy" table="t_teambuy" >
        <id name="id" column="id" unsaved-value="0">
            <generator class="identity" />
        </id>
        <property name="goodsId" column="goods_id" type="java.lang.Long" ></property>
        <property name="title" column="title" type="java.lang.String" ></property>
        <property name="status" column="status" type="java.lang.Integer" ></property>
        <property name="storeId" column="store_id" type="java.lang.Long" ></property>
        <property name="people" column="people" type="java.lang.String" ></property>
        <property name="specs" column="specs" type="java.lang.String" ></property>
    </class>
</hibernate-mapping>